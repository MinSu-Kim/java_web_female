<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.yi.java_web_female.dao.EmployeeMapper">

	<!-- selectTitleByNo -->
	<select id="selectEmployeeByNo" parameterType="Employee" resultType="Employee">
		select code, name, phone, passwd, t_code as tCode 
		from employee
		where code=#{code}
	</select>

	<!-- selectEmployeeByAll -->
	<select id="selectEmployeeByAll" resultType="Employee">
		select code, name, phone, passwd, t_code as tCode 
		from employee
	</select> 


	<!-- selectEmployeeById -->
	<select id="selectEmployeeById" resultType="Employee">
		select code, name, phone, passwd, t_code as tCode 
		from employee
		where code=#{code}
	</select>
	
	
	<!-- selectEmployeeByPw -->
	<select id="selectEmployeeByPw" resultType="Employee">
		select code, name, phone, passwd, t_code as tCode 
		from employee
		where code=#{code} and passwd = password(#{passwd});
	</select>
	
	
