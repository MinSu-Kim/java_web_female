<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.yi.java_web_female.dao.AddOptionMapper">


	<select id="selectAddOptionByNo" parameterType="AddOption" resultType="AddOption">
		select option_id, car_code
		from add_option
		where option_id=#{optionId}
	</select>
	
	<select id="selectAddOptionByAll" resultType="AddOption">
		select option_id, car_code
		from add_option
	</select>

</mapper>