<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.yi.java_web_female.dao.BrandMapper">

	<select id="selectStudentByNo" parameterType="Brand" resultType="Brand">
		select no, name from brand where no=#{no};
	</select>
	
	<select id="selectBrandByAll">
		select no, name from brand;
	</select>
</mapper>